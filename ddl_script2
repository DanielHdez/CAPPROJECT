
CREATE TABLE com_dhl_Products (
  ID NVARCHAR(36) NOT NULL,
  createdAt TIMESTAMP_TEXT,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP_TEXT,
  modifiedBy NVARCHAR(255),
  Name NVARCHAR(5000) NOT NULL,
  Description NVARCHAR(5000),
  ImageUrl NVARCHAR(5000),
  ReleaseDate TIMESTAMP_TEXT DEFAULT CURRENT_TIMESTAMP,
  DiscontinuedDate TIMESTAMP_TEXT,
  Price DECIMAL(16, 2),
  Height DECIMAL(16, 2),
  Width DECIMAL(16, 2),
  Depth DECIMAL(16, 2),
  Quantity DECIMAL(16, 2),
  Supplier_ID NVARCHAR(36),
  PriceCondition NVARCHAR(2),
  PriceDetermination NVARCHAR(3),
  PRIMARY KEY(ID)
);

CREATE TABLE com_dhl_Suppliers (
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(5000),
  Street NVARCHAR(5000),
  City NVARCHAR(5000),
  State NVARCHAR(5000),
  PostalCode NVARCHAR(5),
  Country NVARCHAR(3),
  Email NVARCHAR(5000),
  Phone NVARCHAR(5000),
  Fax NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_dhl_StockAvility (
  ID INTEGER NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_dhl_Currency (
  ID NVARCHAR(3) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_dhl_UnitOfMesurejection (
  ID NVARCHAR(3) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_dhl_DimensionUnits (
  ID NVARCHAR(3) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_dhl_Months (
  ID NVARCHAR(3) NOT NULL,
  Description NVARCHAR(5000),
  ShortDescription NVARCHAR(3),
  PRIMARY KEY(ID)
);

CREATE TABLE com_dhl_ProductReview (
  Name NVARCHAR(3) NOT NULL,
  Rating INTEGER,
  comment NVARCHAR(5000),
  PRIMARY KEY(Name)
);

CREATE TABLE com_dhl_SalesData (
  ID NVARCHAR(36) NOT NULL,
  DeliveryDate DATE_TEXT,
  Revenue DECIMAL(16, 2),
  PRIMARY KEY(ID)
);

CREATE VIEW com_dhl_SelProducts AS SELECT
  Products_0.ID,
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination
FROM com_dhl_Products AS Products_0;

CREATE VIEW com_dhl_SelProducts1 AS SELECT
  Products_0.ID,
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination
FROM com_dhl_Products AS Products_0;

CREATE VIEW com_dhl_SelProducts2 AS SELECT
  Products_0.Name,
  Products_0.Price,
  Products_0.Quantity
FROM com_dhl_Products AS Products_0;

CREATE VIEW com_dhl_SelProducts3 AS SELECT
  ProductReview_1.Rating,
  Products_0.Name,
  sum(Products_0.Price) AS TotalPrice
FROM (com_dhl_Products AS Products_0 LEFT JOIN com_dhl_ProductReview AS ProductReview_1 ON Products_0.Name = ProductReview_1.Name)
GROUP BY ProductReview_1.Rating, Products_0.Name
ORDER BY Rating;

CREATE VIEW com_dhl_ProjProducts AS SELECT
  Products_0.ID,
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination
FROM com_dhl_Products AS Products_0;

CREATE VIEW com_dhl_ProjProducts2 AS SELECT
  Products_0.ID,
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination
FROM com_dhl_Products AS Products_0;

CREATE VIEW com_dhl_ProjProducts3 AS SELECT
  Products_0.ReleaseDate,
  Products_0.Name
FROM com_dhl_Products AS Products_0;

