FROM (com_dhl_DimensionUnits AS L_0 LEFT JOIN com_dhl_DimensionUnits_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_dhl_Months AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description,
  coalesce(localized_1.ShortDescription, L_0.ShortDescription) AS ShortDescription
FROM (com_dhl_Months AS L_0 LEFT JOIN com_dhl_Months_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

